{ 
	"embed": {
		"author": {
			"name": "${user_tag} aka ${guildMember_nickname}"
		},
		"thumbnail": {
			"url": "${user_avatarURL}"
		},
		"fields": [
			{
				"name": "%{info.uinfo.joinedDiscordAt}",
				"value": "${user_createdAt}"
			},
			{
				"name": "%{info.uinfo.joinedGuildAt}",
				"value": "${guildMember_joinedAt}"
			}
		]
	}
}
