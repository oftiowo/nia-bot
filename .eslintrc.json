{
	"env": {
			"es6": true,
			"node": true
	},
	"extends": "eslint:recommended",
	"rules": {
			"no-console": "off",
			"no-unused-vars":"warn",
			"eol-last":"warn",
			"indent": [
					"error",
					"tab"
			],
			"quotes": [
					"error",
					"backtick"
			],
			"semi": [
					"error",
					"always"
			]
	}
}